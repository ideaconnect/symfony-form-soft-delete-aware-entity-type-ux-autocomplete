{
    "name": "idct/symfony-form-soft-delete-aware-entity-type-ux-autocomplete",
    "description": "symfony/ux-autocomplete addon to the SoftDeleteAwareEntityType which adds support for Soft Delete filter behavior to Symfony's EntityType.",
    "type": "library",
    "license": "MIT",
    "autoload-dev": {
        "psr-4": {
            "IDCT\\Tests\\SymfonyFormSoftDeleteAwareEntityType\\Ux\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "IDCT\\SymfonyFormSoftDeleteAwareEntityType\\Ux\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Bartosz Pacho≈Çek",
            "email": "bartosz@idct.tech"
        }
    ],
    "scripts": {
        "fix": [
            "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --allow-risky=yes"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "idct/symfony-form-soft-delete-aware-entity-type": "^1.0.2",
        "symfony/ux-autocomplete": "^2",
        "symfony/routing": "^6 || ^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "friendsofphp/php-cs-fixer": "^3.59"
    }
}
